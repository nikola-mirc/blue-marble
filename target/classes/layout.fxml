<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane maxHeight="-Infinity" maxWidth="-Infinity"
	minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0"
	prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1"
	xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="hellofx.BlueMarbleController">
	<children>
		<ImageView fx:id="image" fitHeight="375.0" fitWidth="389.0"
			layoutX="14.0" layoutY="14.0" pickOnBounds="true"
			preserveRatio="true" />
		<DatePicker fx:id="datePicker" layoutX="421.0"
			layoutY="17.0" onAction="#updateDate" prefHeight="32.0"
			prefWidth="106.0" />
		<Button fx:id="enhanceButton" visible="false" layoutX="448.0"
			layoutY="84.0" mnemonicParsing="false" onAction="#enhanceImage"
			text="Enhance" />
		<Button fx:id="colorAdjustButton" visible="false"
			layoutX="430.0" layoutY="132.0" mnemonicParsing="false"
			onAction="#colorAdjust" text="Black &amp; white" />
	</children>
</AnchorPane>
